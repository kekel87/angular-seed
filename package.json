{
  "name": "seed",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npm run lint:ng && npm run lint:prettier && npm run lint:stylelint ",
    "lint:ng": "ng lint",
    "lint:prettier": "prettier --check \"./@(src|e2e)/**/*.@(js|scss|json)\"",
    "lint:stylelint": "stylelint \"src/**/*.scss\"",
    "format": "npm run format:ng && npm run format:prettier && npm run format:stylelint",
    "format:ng": "ng lint --fix",
    "format:prettier": "prettier --write \"./@(src|e2e)/**/*.@(js|scss|json)\"",
    "format:stylelint": "stylelint \"src/**/*.scss\" --fix",
    "e2e": "ng e2e"
  },
  "private": true,
